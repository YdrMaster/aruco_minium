    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb)aruco_test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/utils/aruco_test",
            "args": ["net-http://admin:admin@192.168.3.76:8081/video",
                     "-c",
                     "${workspaceFolder}/build/utils/out_camera_calibration.yml",
                     "-s",
                     "0.03"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb)aruco_test_markermap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/utils_markermap/aruco_test_markermap",
            "args": ["net-http://admin:admin@192.168.3.76:8081/video"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}